  "scripts": {
    "dev:local": "cross-env NODE_ENV=development nodemon  ./src/app.js  --watch src/", 
    "dev:local:common":"本地开发调试，启动一个进程",
    "dev": "cross-env NODE_ENV=production --max_old_space_size=2048 nodemon  ./src/app.js  --watch src/",
    "dev:common":"生产前台启动，启动多进程、进程守护",
    "start": "cross-env NODE_ENV=production pm2 start ./pm2.json",
    "start:common": "生产后台启动，启动多进程、进程守护",
    "status": "pm2 status",
    "reload": "pm2 reload ./pm2.json",
    "kill": "pm2 kill -n amcPdfServer",
    "stop": "pm2 stop amcPdfServer"
  },
